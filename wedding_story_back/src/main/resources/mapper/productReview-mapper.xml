<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.iei.product.dao.ProductReviewDao">
	<insert id="insertReview">
		insert into product_comment values(product_comment_seq.nextval,#{product.productNo},#{payNo},#{memberNo},#{rating},#{review},to_char(sysdate,'yy/mm/dd'))
	</insert>
</mapper>
