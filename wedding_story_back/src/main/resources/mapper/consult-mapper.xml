<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.iei.consult.model.dao.ConsultDao">
		
		<insert id="insertConsult">
			insert into consult values(consult_seq.nextval,#{memberNo},#{productNo},#{consultDate},#{consultTime},#{reservation},#{consultTitle},#{consultWrite})
		</insert>
		
		<select id="selectConsult" resultType="consult">
			select * from consult
		</select>
		
		<select id="selectConsultList" resultType="consult">
			select * from consult 
			where product_no in 
			<foreach collection="productNo" item="No" open="(" close=")" separator=",">
				#{No}
			</foreach> 
		</select>
</mapper>
